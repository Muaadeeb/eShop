@page "/outstandingOrders"
@page "/admin"

    @attribute [Authorize]

    @inject IViewOutstandingOrdersUseCase _viewOutstandingOrdersUseCase

    <h3>OutstandingOrdersComponent</h3>
    <br />
    <OrderListComponent Orders="_orders"></OrderListComponent>

    @code {

        private IEnumerable<Order> _orders;

        protected override void OnInitialized()
        {
            base.OnInitialized();

            _orders = _viewOutstandingOrdersUseCase.Execute();
        }
    }
