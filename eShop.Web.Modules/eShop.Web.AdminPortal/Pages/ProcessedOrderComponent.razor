@page "/processedorders"

@attribute [Authorize]

@inject IViewProcessedOrdersUseCase _viewProcessedOrdersUseCase

<h3>Processed Orders</h3>

<br />
<OrderListComponent Orders="_orders"></OrderListComponent>

@code {

    private IEnumerable<Order> _orders;

    protected override void OnInitialized()
    {
        base.OnInitialized();

        _orders = _viewProcessedOrdersUseCase.Execute();
    }
}
